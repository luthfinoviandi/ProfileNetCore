@{
    ViewData["Title"] = "Dashboard";
    KTTheme.AddVendors(new string[] { "amcharts", "amcharts-maps", "amcharts-stock" });
}
<!--begin::Row-->
<div class="row gx-5 gx-xl-10">
    <div class="col-xxl-6">
        <h2>Profile Summary</h2>
    </div>
    <div class="col-xxl-6">
        <button type="button" class="btn btn-sm btn-info float-end" onclick="sendMessage()"><i class="fa fa-paper-plane"></i>Send
            Me a Message</button>
    </div>
    <!--begin::Col-->
    <div class="col-xxl-4 mb-5 mb-xl-10 my-5">
        <partial name="~/Views/Pages/Dashboards/Partials/SkillSummary.cshtml" />
    </div>
    <!--end::Col-->
    <!--begin::Col-->
    <div class="col-xxl-4 mb-5 mb-xl-10 my-5">
        <partial name="~/Views/Pages/Dashboards/Partials/TechnicalSkill.cshtml" />
    </div>
    <!--end::Col-->
    <!--begin::Col-->
    <div class="col-xxl-4 mb-5 mb-xl-10 my-5">
        <partial name="~/Views/Pages/Dashboards/Partials/LanguageSkill.cshtml" />
    </div>
    <!--end::Col-->
</div>
<!--end::Row-->
<!--begin::Row-->
<div class="row gx-5 gx-xl-10">
    <div class="col-xxl-12 mb-5 mb-xl-10">
        <partial name="~/Views/Pages/Dashboards/Partials/Experience.cshtml" />
    </div>
</div>
<!--end::Row-->

<div class="modal fade" id="sendMessageModal" tabindex="-1" aria-modal="true" role="dialog">
    <!--begin::Modal dialog-->
    <div class="modal-dialog mw-700px">
        <!--begin::Modal content-->
        <div class="modal-content">
            <!--begin::Modal header-->
            <div class="modal-header  pb-0 border-0 d-flex justify-content-end">
                <!--begin::Close-->
                <div class="btn btn-sm btn-icon btn-active-color-primary" data-bs-dismiss="modal">
                    <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
                </div>
                <!--end::Close-->
            </div>
            <!--end::Modal header-->

            <!--begin::Modal body-->
            <div class="modal-body scroll-y mx-5 mx-xl-10 pt-0 pb-15">
                <div class="d-flex flex-column mb-8 fv-row fv-plugins-icon-container">
                    <!--begin::Label-->
                    <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                        <span>Subject</span>
                    </label>
                    <!--end::Label-->

                    <input type="text" class="form-control form-control-solid" placeholder="Enter Subject" name="target_title">
                </div>
                <div class="d-flex flex-column mb-8 fv-row fv-plugins-icon-container">
                    <!--begin::Label-->
                    <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                        <span>Message</span>
                    </label>
                    <!--end::Label-->

                    <textarea class="form-control form-control-solid" placeholder="Enter Message" rows="9"></textarea>
                </div>
                <div class="text-center">
                    <button type="reset" data-bs-dismiss="modal" class="btn btn-light me-3">
                        Cancel
                    </button>

                    <button type="submit" onclick="doSend()" class="btn btn-primary">
                        <span class="indicator-label">
                            Submit
                        </span>
                    </button>
                </div>
            <!--end::Modal Body-->
            </div>
        <!--end::Modal content-->
        </div>
    <!--end::Modal dialog-->
    </div>
</div>

@{
  KTTheme.AddJavascriptFile("/js/custom/dashboards/index.js");
}